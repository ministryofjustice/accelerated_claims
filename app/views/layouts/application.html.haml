- content_for :page_title do
  = "#{@page_title} - #{Rails.configuration.app_title}"

- content_for :stylesheets do
  = stylesheet_link_tag "application", media: "all"
  :erb
    <!--[if IE 6]> <%= stylesheet_link_tag "ie6", media: "all" %> <![endif]-->
    <!--[if IE 7]> <%= stylesheet_link_tag "ie7", media: "all" %> <![endif]-->
    <!--[if IE 8]> <%= stylesheet_link_tag "ie8", media: "all" %> <![endif]-->

- content_for :javascripts do
  :erb
    <!--[if gte IE 7]><!--> <%= javascript_include_tag "application" %> <!--<![endif]-->

- content_for :head do
  = csrf_meta_tag
  = render partial: 'shared/analytics'

- content_for :cookie_message, ""



- content_for :content do
  .phase-banner
    %p
      %span This is a new service - #{link_to "your feedback", config_item(:feedback_url)} will help us to improve it.  If you need help with using the online service, we can offer #{link_to 'technical help', technical_help_path}.
  =yield

- content_for :footer_app_links do
  %li
    %ul
      %li= link_to 'Help', :help
      %li= link_to 'Cookies', :cookies

- if Rails.env.production?
  - content_for :body_end do
    %script{type: 'text/javascript', charset: 'utf-8', src: 'https://cdn3.userzoom.com/uz.js?cuid=13AFA5DAF5D9E311BEDA0022196C4538'}


= render template: "layouts/moj_template"

  
